    "babel-root-slash-import": "1.x.x",
    "chai": "3.x.x",
    "enzyme": "^2.0.0",
    "eslint": "3.16.x",
    "eslint-plugin-react": "6.10.x",
    "mocha": "3.x.x",
    "react-addons-test-utils": "^15.4.2",
    "sinon": "1.17.x",
    "jsdom": "^9.11.0"
  },
  "dependencies": {
